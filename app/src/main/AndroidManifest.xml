<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.funhouse">

    <application
        android:name=".WeatherApp.WeatherApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.FunHouse"
        android:usesCleartextTraffic="true">
        <activity android:name=".WeatherApp.WeatherActivity"></activity>
        <activity android:name=".WallpaperSpawner.WallpaperActivity"></activity>
        <activity android:name=".JailHouse.JailHouseRock" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

    <uses-permission android:name="android.permission.INTERNET" />

</manifest>